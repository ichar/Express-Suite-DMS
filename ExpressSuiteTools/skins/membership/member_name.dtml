<dtml-let mode="not _.has_key('mode') and 'LMF' or mode" IsNoOwner="(not member or member.find('No owner')) > -1 and 1 or 0" member="IsNoOwner and msg('No owner') or member"><dtml-if IsNoOwner><nobr><dtml-var member></nobr><dtml-elif "portal_membership.isAnonymousUser()"><dtml-var "portal_membership.getMemberName(member)"><dtml-elif "member.startswith('group:')"><dtml-let group="member[6:]"><a OnClick="return OpenGroupInfoWnd('&dtml-group;','')" style="text-decoration:underline;cursor:hand;"><dtml-var "portal_membership.getGroupTitle(group)"></a></dtml-let><dtml-else ><a OnClick="return OpenUserInfoWnd('&dtml-member;','')" style="text-decoration:underline;cursor:hand;"><dtml-if brief><dtml-var "portal_membership.getMemberBriefName(member, mode)"><dtml-else><dtml-var "portal_membership.getMemberName(member)"></dtml-if></a></dtml-if></dtml-let>
